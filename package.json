{
  "name": "my-setup-ts",
  "version": "0.1.0",
  "private": true,
  "homepage": "../",
  "dependencies": {
    "@types/history": "^4.6.2",
    "@types/react-router": "^4.0.23",
    "es6-promise": "^4.2.4",
    "isomorphic-fetch": "^2.2.1",
    "node-sass-chokidar": "^1.1.2",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-materialize": "^2.1.4",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.14.0",
    "react-simple-pie-chart": "^0.5.0"
  },
  "scripts": {
    "start-ts": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-ts",
    "build-ts": "react-scripts-ts build",
    "build": "npm-run-all build-css build-ts",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.7",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "jest": "^22.4.2",
    "ts-jest": "^22.4.2",
    "typescript": "^2.7.2"
  }
}
